class Solution {
    public int[] shuffle(int[] nums, int n) {
      
        // work on this please one -->>attention
        
        int[] ans=new int[2*n];
        for(int i=0,j=n,index=0;i<ans.length && index<ans.length;i++,j++){
          ans[index++]=nums[i];
          ans[index++]=nums[j];
        }
        return ans;
        
    }
}
